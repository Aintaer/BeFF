{
  "strict": true,
  "unused": true,

  //move to JSCS eventually
  "curly": true,
  "trailing": true,
  "newcap": true,

  "maxcomplexity": 7,
  "maxstatements": 20,

  "boss": true,
  "browser": true,
  "eqeqeq": true,
  "eqnull": true,
  "expr": true,
  "immed": true,
  "latedef": true,
  "undef": true,
  "unused": "vars",
  "globals": {
    "require": false,
    "define": false
  }
}
